@model IEnumerable<MVCWardrobeProject.Accessory>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.AccessoryName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AccessoryPhoto)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AcessoryColor)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AccessoryType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AccessoryOccasion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AccesorySeason)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.AccessoryName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AccessoryPhoto)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AcessoryColor)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AccessoryType)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AccessoryOccasion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AccesorySeason)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.AccessoryID }) |
            @Html.ActionLink("Details", "Details", new { id=item.AccessoryID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.AccessoryID })
        </td>
    </tr>
}

</table>
